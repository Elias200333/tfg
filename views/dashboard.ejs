<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags-->
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="au theme template">
  <meta name="author" content="Hau Nguyen">
  <meta name="keywords" content="au theme template">

  <!-- Title Page-->
  <title>Dashboard  | PermaHosting</title>

  <!-- Fontfaces CSS-->
  <link href="/css/font-face.css" rel="stylesheet" media="all">
  <link href="/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
  <link href="/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
  <link href="/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

  <!-- Bootstrap CSS-->
  <link href="/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

  <!-- Vendor CSS-->
  <link href="/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
  <link href="/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
  <link href="/vendor/wow/animate.css" rel="stylesheet" media="all">
  <link href="/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
  <link href="/vendor/slick/slick.css" rel="stylesheet" media="all">
  <link href="/vendor/select2/select2.min.css" rel="stylesheet" media="all">
  <link href="/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

  <!-- Main CSS-->
  <link href="/css/theme.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
  <div class="page-wrapper">
    <!-- HEADER MOBILE-->
    <header class="header-mobile d-block d-lg-none">
      <div class="header-mobile__bar">
        <div class="container-fluid">
          <div class="header-mobile-inner">
            <a class="logo" href="/">
              <img width="40" src="/images/icon/logo.png" alt="PermaHosting" />
            </a>
            <button class="hamburger hamburger--slider" type="button">
              <span class="hamburger-box">
                <span class="hamburger-inner"></span>
              </span>
            </button>
          </div>
        </div>
      </div>
      <nav class="navbar-mobile">
        <div class="container-fluid">
          <ul class="navbar-mobile__list list-unstyled">
            <li class="active has-sub">
              <a class="js-arrow" href="#">
                <i class="fas fa-tachometer-alt"></i>Dashboard</a>
            </li>
            <li>
              <a href="/dashboard/proyects/1">
                <i class="fas fa-table"></i>My projects</a>
            </li>
            <li>
              <a href="/dashboard/create">
                <i class="fas fa-plus"></i>Create proyect</a>
            </li>
            <li>
              <a href="/dashboard/update">
                <i class="fas fa-sync-alt"></i>Update proyect</a>
            </li>
          </ul>
        </div>
      </nav>
    </header>
    <!-- END HEADER MOBILE-->

    <!-- MENU SIDEBAR-->
    <aside class="menu-sidebar d-none d-lg-block">
      <div class="logo">
        <a href="#" style="color: #675280; font-size: 20px; font-family: sans-serif; font-weight: 600;">
          <img width="40" src="/images/icon/logo.png" alt="PermaHosting" /> PERMAHOSTING
        </a>
      </div>
      <div class="menu-sidebar__content js-scrollbar1">
        <nav class="navbar-sidebar">
          <ul class="list-unstyled navbar__list">
            <li class="active has-sub">
              <a class="js-arrow" href="#">
                <i class="fas fa-tachometer-alt"></i>Dashboard</a>
            </li>
            <li>
              <a href="/dashboard/proyects/1">
                <i class="fas fa-table"></i>My projects</a>
            </li>
            <li>
              <a href="/dashboard/create">
                <i class="fas fa-plus"></i>Create proyect</a>
            </li>
            <li>
              <a href="/dashboard/update">
                <i class="fas fa-sync-alt"></i>Update proyect</a>
            </li>
          </ul>
        </nav>
      </div>
    </aside>
    <!-- END MENU SIDEBAR-->

    <!-- PAGE CONTAINER-->
    <div class="page-container">
      <!-- HEADER DESKTOP-->
      <header class="header-desktop">
        <div class="section__content section__content--p30">
          <div class="container-fluid">
            <div class="header-wrap">
              <form class="form-header" action="/dashboard/search" method="POST">
                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search proyects..." />
                <button class="au-btn--submit" type="submit">
                  <i class="zmdi zmdi-search"></i>
                </button>
              </form>
              <div class="header-button">
                <div class="account-wrap">
                  <div class="account-item clearfix js-item-menu">
                    <div class="image">
                      <img src="/images/icon/avatar-01.jpg" alt="<%= nombre %>" />
                    </div>
                    <div class="content">
                      <a class="js-acc-btn" href="#">
                        <%= nombre %>
                      </a>
                    </div>
                    <div class="account-dropdown js-dropdown">
                      <div class="info clearfix">
                        <div class="image">
                          <a href="#">
                            <img src="/images/icon/avatar-01.jpg" alt="<%= nombre %>" />
                          </a>
                        </div>
                        <div class="content">
                          <h5 class="name">
                            <a href="#">
                              <%= nombre %>
                            </a>
                          </h5>
                          <span class="email">
                            <%= email %>
                          </span>
                        </div>
                      </div>
                      <div class="account-dropdown__body">
                        <div class="account-dropdown__item">
                          <a href="#">
                            <i class="zmdi zmdi-account"></i>Account</a>
                        </div>
                      </div>
                      <div class="account-dropdown__footer">
                        <a href="/logout">
                          <i class="zmdi zmdi-power"></i>Logout</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <!-- HEADER DESKTOP-->

      <!-- MAIN CONTENT-->
      <div class="main-content">
        <div class="section__content section__content--p30">
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-12">
                <div class="overview-wrap">
                  <h2 class="title-1">overview</h2>
                  <a href="/dashboard/create" class="au-btn au-btn-icon au-btn--blue">
                    <i class="zmdi zmdi-plus"></i>new proyect</a>
                </div>
              </div>
            </div>
            <div class="row m-t-25">
              <div class="col-sm-6 col-lg-3">
                <div class="overview-item overview-item--c1">
                  <div class="overview__inner">
                    <div class="overview-box clearfix">
                      <div class="icon">
                        <i class="zmdi zmdi-account-o"></i>
                      </div>
                      <div class="text">
                        <h2>
                          <% if (estadisticas.TotalVisits) { %>
                            <%= estadisticas.TotalVisits %>
                          <% } else {%>
                              0
                          <% } %>
                        </h2>
                        <span>TOTAL VISITS</span>
                      </div>
                    </div>
                    <div class="overview-chart">
                      <canvas id="widgetChart1"></canvas>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-sm-6 col-lg-3">
                <div class="overview-item overview-item--c2">
                  <div class="overview__inner">
                    <div class="overview-box clearfix">
                      <div class="icon">
                        <i class="zmdi zmdi-memory"></i>
                      </div>
                      <div class="text">
                        <h2>
                          <% if (estadisticas.mbUpload) { %>
                            <% if (estadisticas.mbUpload > 0.01) { %>
                              <%= parseFloat(estadisticas.mbUpload).toFixed(2) %>
                            <% } else {%>
                              <%= parseFloat(estadisticas.mbUpload).toFixed(4) %>
                            <% } %>
                          <% } else {%>
                              0
                          <% } %>
                        </h2>
                        <span>MB UPLOAD</span>
                      </div>
                    </div>
                    <div class="overview-chart">
                      <canvas id="widgetChart2"></canvas>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-sm-6 col-lg-3">
                <div class="overview-item overview-item--c3">
                  <div class="overview__inner">
                    <div class="overview-box clearfix">
                      <div class="icon">
                        <i class="zmdi zmdi-collection-plus"></i>
                      </div>
                      <div class="text">
                        <span>MOST POPULAR</span>
                        <h5>
                          <% if (estadisticas.mostPopular) { %>
                            <%= estadisticas.mostPopular %>
                          <% } else {%>
                              Nothing
                          <% } %> 
                        </h5>
                      </div>
                    </div>
                    <div class="overview-chart">
                      <canvas id="widgetChart3"></canvas>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-sm-6 col-lg-3">
                <div class="overview-item overview-item--c4">
                  <div class="overview__inner">
                    <div class="overview-box clearfix">
                      <div class="icon">
                        <i class="zmdi zmdi-money"></i>
                      </div>
                      <div class="text">
                        <h2>
                          <% if (estadisticas.totalInversion) { %>
                            <%= estadisticas.totalInversion %>
                          <% } else {%>
                              0
                          <% } %> 
                        </h2>
                        <span>TOTAL INVERSION</span>
                      </div>
                    </div>
                    <div class="overview-chart">
                      <canvas id="widgetChart4"></canvas>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-lg-9">
                <div class="row">
                  <h2 class="title-1 m-b-25 col-lg-3">PROYECTS</h2>
                </div>
                <% if (proyectos.length > 0) { %>
                  <div class="table-responsive table--no-card m-b-40">
                    <table class="table table-borderless table-striped table-earning">
                      <thead>
                        <tr>
                          <th>NAME</th>
                          <th>ACTUAL SIZE</th>
                          <th>CREATED</th>
                          <th class="text-right">VISITS</th>
                          <th class="text-right">UPDATES</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for (var i=0; i < proyectos.length; i++) { %>
                          
                          <tr>
                            <td>
                              <a class="text-muted" href="/dashboard/info/<%= proyectos[i].ID %>"><%= proyectos[i].nombre %></a>
                            </td>
                            <td>
                              <a class="text-muted" href="/dashboard/info/<%= proyectos[i].ID %>">
                              <% if (parseFloat(proyectos[i].size) > 0.01) { %>
                                <%= parseFloat(proyectos[i].size).toFixed(2) %>
                              <% } else {%>
                                <%= parseFloat(proyectos[i].size).toFixed(4) %>
                              <% } %>
                              </a>
                            </td>
                            <td>
                              <a class="text-muted" href="/dashboard/info/<%= proyectos[i].ID %>"><%= proyectos[i].date_up %></a>
                            </td>
                            <td class="text-right">
                              <a class="text-muted" href="/dashboard/info/<%= proyectos[i].ID %>"><%= proyectos[i].visits %></a>
                            </td>
                            <td class="text-right">
                              <a class="text-muted" href="/dashboard/info/<%= proyectos[i].ID %>"><%= proyectos[i].updates %></a>
                            </td>
                          </tr>
                          <% } %>
                      </tbody>
                    </table>
                  </div>
                  <% } else {%>
                    <h3>TODAVÍA NO HA CREADO NINGÚN PROYECTO. REALÍCELO <a href="/dashboard/create">AQUÍ</a>.</h3>
                    <% } %>
              </div>
              <div class="col-lg-3">
                <h2 class="title-1 m-b-25">TOP PROYECTS</h2>
                <div class="au-card au-card--bg-blue au-card-top-countries m-b-40">
                  <div class="au-card-inner">
                    <% if (top.length> 0) { %>
                      <div class="table-responsive">
                        <table class="table table-top-countries">
                          <tbody>
                            <% for (var i=0; i < top.length; i++) { %>
                              <tr>
                                <td>
                                  <%= top[i].nombre %>
                                </td>
                                <td class="text-right">
                                  <%= top[i].totalVisits %> visits
                                </td>
                              </tr>
                              <% } %>
                          </tbody>
                        </table>
                      </div>
                      <% } else { %>
                        <p class="h6 text-light text-center">NO PROYECTS</p>
                        <% } %>
                  </div>
                </div>
              </div>
            </div>
            <footer class="row">
              <div class="col-md-12">
                <div class="copyright">
                  <p>Copyright © 2023 PermaHosting. All rights reserved.</p>
                </div>
              </div>
            </footer>
          </div>
        </div>
      </div>
      <!-- END MAIN CONTENT-->
      <!-- END PAGE CONTAINER-->
    </div>

  </div>

  <!-- Jquery JS-->
  <script src="/vendor/jquery-3.2.1.min.js"></script>
  <!-- Bootstrap JS-->
  <script src="/vendor/bootstrap-4.1/popper.min.js"></script>
  <script src="/vendor/bootstrap-4.1/bootstrap.min.js"></script>
  <!-- Vendor JS       -->
  <script src="/vendor/slick/slick.min.js">
  </script>
  <script src="/vendor/wow/wow.min.js"></script>
  <script src="/vendor/animsition/animsition.min.js"></script>
  <script src="/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
  </script>
  <script src="/vendor/counter-up/jquery.waypoints.min.js"></script>
  <script src="/vendor/counter-up/jquery.counterup.min.js">
  </script>
  <script src="/vendor/circle-progress/circle-progress.min.js"></script>
  <script src="/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
  <script src="/vendor/chartjs/Chart.bundle.min.js"></script>
  <script src="/vendor/select2/select2.min.js">
  </script>

  <!-- Main JS-->
  <script src="/js/main.js"></script>

</body>

</html>
<!-- end document-->