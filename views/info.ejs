<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags-->
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="au theme template">
  <meta name="author" content="Hau Nguyen">
  <meta name="keywords" content="au theme template">

  <!-- Title Page-->
  <title>My proyects | PermaHosting</title>

  <!-- Fontfaces CSS-->
  <link href="/css/font-face.css" rel="stylesheet" media="all">
  <link href="/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
  <link href="/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
  <link href="/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

  <!-- Bootstrap CSS-->
  <link href="/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

  <!-- Vendor CSS-->
  <link href="/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
  <link href="/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
  <link href="/vendor/wow/animate.css" rel="stylesheet" media="all">
  <link href="/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
  <link href="/vendor/slick/slick.css" rel="stylesheet" media="all">
  <link href="/vendor/select2/select2.min.css" rel="stylesheet" media="all">
  <link href="/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

  <!-- Main CSS-->
  <link href="/css/theme.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
  <div class="page-wrapper">
    <!-- HEADER MOBILE-->
    <header class="header-mobile d-block d-lg-none">
      <div class="header-mobile__bar">
        <div class="container-fluid">
          <div class="header-mobile-inner">
            <a class="logo" href="/">
              <img width="40" src="/images/icon/logo.png" alt="PermaHosting" />
            </a>
            <button class="hamburger hamburger--slider" type="button">
              <span class="hamburger-box">
                <span class="hamburger-inner"></span>
              </span>
            </button>
          </div>
        </div>
      </div>
      <nav class="navbar-mobile">
        <div class="container-fluid">
          <ul class="navbar-mobile__list list-unstyled">
            <li class="active has-sub">
              <a class="js-arrow" href="#">
                <i class="fas fa-tachometer-alt"></i>Dashboard</a>
            </li>
            <li>
              <a href="/dashboard/proyects/1">
                <i class="fas fa-table"></i>My projects</a>
            </li>
            <li>
              <a href="/dashboard/create">
                <i class="fas fa-plus"></i>Create proyect</a>
            </li>
            <li>
              <a href="/dashboard/update">
                <i class="fas fa-sync-alt"></i>Update proyect</a>
            </li>
          </ul>
        </div>
      </nav>
    </header>
    <!-- END HEADER MOBILE-->

    <!-- MENU SIDEBAR-->
    <aside class="menu-sidebar d-none d-lg-block">
      <div class="logo">
        <a href="#" style="color: #675280; font-size: 20px; font-family: sans-serif; font-weight: 600;">
          <img width="40" src="/images/icon/logo.png" alt="PermaHosting" /> PERMAHOSTING
        </a>
      </div>
      <div class="menu-sidebar__content js-scrollbar1">
        <nav class="navbar-sidebar">
          <ul class="list-unstyled navbar__list">
            <li>
              <a class="js-arrow" href="/dashboard">
                <i class="fas fa-tachometer-alt"></i>Dashboard</a>
            </li>
            <li class="active has-sub">
              <a href="#">
                <i class="fas fa-table"></i>My projects</a>
            </li>
            <li>
              <a href="/dashboard/create">
                <i class="fas fa-plus"></i>Create proyect</a>
            </li>
            <li>
              <a href="/dashboard/update">
                <i class="fas fa-sync-alt"></i>Update proyect</a>
            </li>
          </ul>
        </nav>
      </div>
    </aside>
    <!-- END MENU SIDEBAR-->

    <!-- PAGE CONTAINER-->
    <div class="page-container">
      <!-- HEADER DESKTOP-->
      <header class="header-desktop">
        <div class="section__content section__content--p30">
          <div class="container-fluid">
            <div class="header-wrap">
              <form class="form-header" action="/dashboard/search" method="POST">
                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search proyects..." />
                <button class="au-btn--submit" type="submit">
                  <i class="zmdi zmdi-search"></i>
                </button>
              </form>
              <div class="header-button">
                <div class="account-wrap">
                  <div class="account-item clearfix js-item-menu">
                    <div class="image">
                      <img src="/images/icon/avatar-01.jpg" alt="<%= nombre %>" />
                    </div>
                    <div class="content">
                      <a class="js-acc-btn" href="#">
                        <%= nombre %>
                      </a>
                    </div>
                    <div class="account-dropdown js-dropdown">
                      <div class="info clearfix">
                        <div class="image">
                          <a href="#">
                            <img src="/images/icon/avatar-01.jpg" alt="<%= nombre %>" />
                          </a>
                        </div>
                        <div class="content">
                          <h5 class="name">
                            <a href="#">
                              <%= nombre %>
                            </a>
                          </h5>
                          <span class="email">
                            <%= email %>
                          </span>
                        </div>
                      </div>
                      <div class="account-dropdown__body">
                        <div class="account-dropdown__item">
                          <a href="#">
                            <i class="zmdi zmdi-account"></i>Account</a>
                        </div>
                      </div>
                      <div class="account-dropdown__footer">
                        <a href="/logout">
                          <i class="zmdi zmdi-power"></i>Logout</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <!-- HEADER DESKTOP-->

      <!-- MAIN CONTENT-->
      <div class="main-content">
        <div class="section__content section__content--p30">
          <div class="container-fluid">
            <% if (proyectInfo) { %>
              <div class="row">
                <div class="col-md-8">
                  <div class="container">
                    <div class="card">
                      <div class="d-flex justify-content-between card-header">
                        <h5 class="h3">NOMBRE: &nbsp; <%= proyectInfo.Nombre %></h5>
                        <span class="font-italic">CREATED: <%= proyectInfo.date_up %></span>
                      </div>
                      <div class="card-body row">
                        <div class="col-md-8">
                          <img class="border" src="<%= imgWebsite %>" alt="vista previa">
                        </div>
                        <div class="col-md-4">
                          <p class="h6">TOTAL UPDATES: <br> <p class="h1"><%= proyectInfo.times_updated %>/
                            <% if (proyectInfo.total_updates > 0) { %>
                              <%= proyectInfo.total_updates %>
                            <% } else {%>
                                1
                            <% } %>
                          </p></p>
                          <br><br><a class="btn btn-primary" href="https://arweave.net/<%= actualWebsite.id %>"><p class="h5">SEE WEBSITE</p></a>
                        </div>
                      </div>
                    </div>
                  </div>
                  
                </div>
                <div class="col-md-4">
                  <div class="container">
                    <div class="card bg-dark text-white">
                      <img class="card-img" src="<%= '/images/plans/plan-'+proyectInfo.idPlan+'.png' %>" alt="Plan Image">
                      <div class="card-img-overlay">
                      </div>
                    </div>
                    <div class="card">
                      <ul class="list-group list-group-flush">
                        <li class="list-group-item h5">TOTAL VISITS: <%= actualWebsite.visits %></li>
                        <li class="list-group-item h5">ACTUAL SIZE:
                            <% if (parseFloat(actualWebsite.size) > 0.01) { %>
                              <%= parseFloat(actualWebsite.size).toFixed(2) %>
                            <% } else {%>
                              <%= parseFloat(actualWebsite.size).toFixed(4) %>
                            <% } %> MB
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-12">
                  <!-- DATA TABLE -->
                  <div class="table-data__tool">
                    <div class="table-data__tool-left">
                      <h3 class="title-5 m-b-35">VERSION HISTORY</h3>
                    </div>
                    <% if (proyectInfo.can_update > 0) { %>
                      <div class="table-data__tool-right">
                        <a href="/dashboard/update/<%= proyectInfo.ID %>" class="au-btn au-btn-icon au-btn--green au-btn--small text-light">
                          <i class="fas fa-sync-alt"></i>UPDATE</a>
                      </div>
                    <% } %>
                  </div>
                  <div class="table-responsive table-responsive-data2">
                    <table class="table table-data2">
                      <thead>
                        <tr>
                          <th>Name</th>
                          <th>Size</th>
                          <th>URL</th>
                          <th>Created</th>
                          <th>Visits</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for (var i=0; i < websitesInfo.length; i++) { %>
                          <tr class="tr-shadow">
                            <td><%= websitesInfo[i].nombre %></td>
                            <td>
                              <span class="block-email">
                                <% if (parseFloat(websitesInfo[i].size) > 0.01) { %>
                                  <%= parseFloat(websitesInfo[i].size).toFixed(2) %>
                                <% } else {%>
                                  <%= parseFloat(websitesInfo[i].size).toFixed(4) %>
                                <% } %> MB
                              </span>
                            </td>
                            <td><a href="https://arweave.net/<%= websitesInfo[i].id %>">Go Website</a></td>
                            <td><%= websitesInfo[i].date_time %></td>
                            <td>
                              <span class="status--process"><%= websitesInfo[i].visits %></span>
                            </td>
                          </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                  <!-- END DATA TABLE -->
                </div>
              </div>
              <% } else {%>
                <div class="row">
                  <p class="col-2 offset-5 h2 text-center">LO SENTIMOS. NO HAY INFORMACIÓN SOBRE EL PROYECTO
                    SELECCIONADO</p>
                </div>
              <% } %>
          </div>
        </div>
        <footer class="fixed-bottom row">
          <div class="text-center col-4 offset-5">
            <div class="copyright">
              <p>Copyright © 2023 PermaHosting. All rights reserved.</p>
            </div>
          </div>
        </footer>
      </div>
    </div>
  </div>
  <!-- END MAIN CONTENT-->
  <!-- END PAGE CONTAINER-->
  </div>

  </div>

  <!-- Jquery JS-->
  <script src="/vendor/jquery-3.2.1.min.js"></script>
  <!-- Bootstrap JS-->
  <script src="/vendor/bootstrap-4.1/popper.min.js"></script>
  <script src="/vendor/bootstrap-4.1/bootstrap.min.js"></script>
  <!-- Vendor JS       -->
  <script src="/vendor/slick/slick.min.js">
  </script>
  <script src="/vendor/wow/wow.min.js"></script>
  <script src="/vendor/animsition/animsition.min.js"></script>
  <script src="/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
  </script>
  <script src="/vendor/counter-up/jquery.waypoints.min.js"></script>
  <script src="/vendor/counter-up/jquery.counterup.min.js">
  </script>
  <script src="/vendor/circle-progress/circle-progress.min.js"></script>
  <script src="/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
  <script src="/vendor/chartjs/Chart.bundle.min.js"></script>
  <script src="/vendor/select2/select2.min.js">
  </script>

  <!-- Main JS-->
  <script src="/js/main.js"></script>

</body>

</html>
<!-- end document-->